<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Select Specification"
		supportMultiselect="false"
		key="specificationID"
		descriptionKey="specificationDescription"
		supportRanges="true">
		<filterBar>
			<fb:FilterBar advancedMode="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="specificationID"
						label="{i18n>SpecificationID}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="specificationID" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="specificationDescription"
						label="{i18n>SpecificationDescription}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="specificationDescription" />
						</fb:control>
					</fb:FilterGroupItem>

					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="createdBY"
						label="{i18n>CreatedBy}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="createdBY" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="specType"
						label="{i18n>SpecificationType}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="specType" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
