<core:FragmentDefinition
	xmlns:vd ="sap.ui.comp.valuehelpdialog"
	xmlns="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core"
	>
	<vd:ValueHelpDialog
		title="{i18n>purpTitle}"
		ok=".onPurposeVHOkPress"
		cancel=".onPurposeVHCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="true"
		key="ID"
		descriptionKey="purpose"
	>
		<vd:filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterPurposeSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="purposeGroup"
						name="purpose"
						label="{i18n>purpose}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<Input name="purpose" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="purposeGroup"
						name="activationStatus"
						label="{i18n>activationStatus}"
						visibleInFilterBar="true"
					>
						<fb:control>
							<Input name="{i18n>activationStatus}" />
						</fb:control>
					</fb:FilterGroupItem>

				
				</fb:filterGroupItems>
			</fb:FilterBar>
		</vd:filterBar>
	</vd:ValueHelpDialog>
</core:FragmentDefinition>
