<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="handleConfirm" title= "Select Category">
		<filterItems>
			<ViewSettingsFilterItem text="Basic" key="0" items="{/oRecipes}">
				<items>
					<ViewSettingsItem text="{subCategoryName}" key="{ID}" selected="{selected}" enabled="{selectable}" itemPropertyChanged="plantSelected"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>